{
  "kind": "collectionType",
  "collectionName": "categories",
  "info": {
    "singularName": "category",
    "pluralName": "categories",
    "displayName": "category"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "categoryName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "private": true,
      "required": true,
      "unique": true
    },
    "totalGood": {
      "type": "integer",
      "min": 0
    },
    "goods": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::good.good",
      "mappedBy": "category_id"
    },
    "types": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::type.type",
      "mappedBy": "category"
    }
  }
}
